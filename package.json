{
  "name": "audit-app",
  "version": "0.1.1",
  "private": true,
  "main": "./dist/main/main.js",
  "author": "<MC/>",
  "description": "Youtube video converter",
  "license": "MIT License",
  "scripts": {
    "restart": "pkill -f 'node server.js' && npm run dev",
    "dev:renderer": "next dev",
    "dev:electron": "electron .",
    "build:renderer": "next build",
    "build:main": "rimraf dist/main && tsc -p tsconfig.main.json",
    "build:preload": "rimraf dist/preload && tsc -p tsconfig.preload.json && copyfiles -u 1 src/main/preload.js dist/preload/",
    "build": "npm run clean && npm run build:main && npm run build:preload && npm run build:renderer && npm run postbuild",
    "clean": "rimraf dist",
    "postbuild": "node -e \"const fs = require('fs'); fs.mkdirSync('dist/preload', { recursive: true }); fs.writeFileSync('dist/preload/package.json', JSON.stringify({ type: 'commonjs' })); fs.mkdirSync('dist/main', { recursive: true }); fs.writeFileSync('dist/main/package.json', JSON.stringify({ type: 'commonjs' }));\"",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && npm run dev:electron\"",
    "start": "electron-forge start",
    "setup:python": "node setup-python.js",
    "setup:ffmpeg": "node setup-ffmpeg.js",
    "prebuild": "npm run setup:python && npm run setup:ffmpeg",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:all": "npm run build && npm run make",
    "export:renderer": "next build && next export -o dist/exported"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^3.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "formidable": "^3.5.4",
    "lucide-react": "^0.544.0",
    "next": "15.5.7",
    "ogl": "^1.0.11",
    "ogl-typescript": "^0.1.40",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/electron": "^1.6.12",
    "@types/node": "^20.19.20",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/webpack": "^5.28.5",
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "crypto-browserify": "^3.12.1",
    "cssnano": "^7.1.2",
    "electron": "^28.3.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "fs-extra": "^11.3.2",
    "https-browserify": "^1.0.0",
    "node-libs-browser": "^2.2.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "url": "^0.11.4",
    "wait-on": "^9.0.1",
    "webpack": "^5.102.1"
  }
}
